{
  "name": "simkey",
  "displayName": "Simkey",
  "publisher": "Border-L",
  "description": "Simkey syntax highlighting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "files.associations": {
    "*.simkey": "simkey"
  },
  "contributes": {
    "languages": [{
      "id": "simkey",
      "aliases": ["Simple Key", "simkey"],
      "extensions": [".simkey"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./assets/main_logo.png",
        "dark": "./assets/main_logo.png"
      }
    },
    {
      "id": "keyc",
      "aliases": ["Key Compiled", "keyc"],
      "extensions": [".keyc"],
      "icon": {
        "light": "./assets/main_logo_keyc.png",
        "dark": "./assets/main_logo_keyc.png"
      }
    }],
    "grammars": [{
      "language": "simkey",
      "scopeName": "source.simkey",
      "path": "./syntaxes/simkey.tmLanguage.json"
    }]
  }
}
